        <!-- Niveles Educativos -->
        <record id="education_level_inicial" model="school.education.level">
            <field name="name">Inicial</field>
            <field name="sequence">10</field>
        </record>
        
        <record id="education_level_primaria" model="school.education.level">
            <field name="name">Primaria</field>
            <field name="sequence">20</field>
        </record>
        
        <record id="education_level_media_general" model="school.education.level">
