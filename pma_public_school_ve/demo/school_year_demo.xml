        <!-- 1. AÑO ESCOLAR ACTUAL                          -->
        <!-- ============================================== -->
        <record id="school_year_2024_2025" model="school.year">
            <field name="name">2024-2025</field>
            <field name="start_date_real">2024-09-16</field>
            <field name="state">active</field>
            <field name="current" eval="True"/>
            <field name="evalution_type_secundary" ref="secundary_100"/>
            <field name="evalution_type_primary" ref="primary_literal"/>
            <field name="evalution_type_pree" ref="pre_observation"/>
        </record>
        
